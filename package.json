    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "scripts/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.config.{js,mjs}": [
      "eslint --fix",
      "prettier --write"
