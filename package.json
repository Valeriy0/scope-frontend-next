  },
  "dependencies": {
    "@fontsource/inter": "^5.2.5",
    "@reduxjs/toolkit": "^2.3.0",
    "@reown/appkit": "^1.7.5",
    "@reown/appkit-adapter-solana": "^1.7.5",
    "@solana/codecs": "^2.1.1",
    "@solana/wallet-adapter-wallets": "^0.19.36",
