    "lint:all:fix": "eslint \"src/**/*.{js,jsx}\" \"pages/**/*.{js,jsx}\" \"scripts/**/*.js\" \"*.config.{js,mjs}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md,css,scss}\"",
    "check": "yarn lint:all && yarn format:check",
